{
    "name": "acadea/cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": ["laravel", "braintree", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Ngu",
            "email": "hello@acadea.com.au"
        }
    ],
    "require": {
        "php": "^7.2.1",
        "braintree/braintree_php": "~3.38",
        "illuminate/contracts": "^6.0.0",
        "illuminate/database": "^6.0.0",
        "illuminate/http": "^6.0.0",
        "illuminate/routing": "^6.0.0",
        "illuminate/support": "^6.0.0",
        "illuminate/view": "^6.0.0",
        "nesbot/carbon": "^2.0",
        "spatie/browsershot": "^3.32",
        "symfony/http-kernel": "~4.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^4.1",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Acadea\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acadea\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Acadea\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
